{
	"name": "@phylum/webpack-electron",
	"version": "1.0.0",
	"description": "Launch electron processes for electron bundled sources",
	"main": "dist/index.js",
	"scripts": {
		"compile": "rimraf dist && tsc",
		"compile:watch": "rimraf dist && tsc -w",
		"test": "npm run compile && nyc -- ava -v",
		"test:watch": "npm run compile && concurrently -n tsc,ava \"tsc -w\" \"ava -w -v\"",
		"prepack": "npm run test && npm run compile"
	},
	"dependencies": {
		"@types/node": "^10.14.4"
	},
	"devDependencies": {
		"@phylum/pipeline": "^4.0.0",
		"@phylum/webpack": "^4.0.2",
		"ava": "^1.4.1",
		"concurrently": "^4.1.0",
		"electron": "^4.1.3",
		"nyc": "^13.3.0",
		"rimraf": "^2.6.3",
		"typescript": "^3.4.1",
		"webpack": "^4.29.6"
	},
	"peerDependencies": {
		"@phylum/pipeline": "^4.0.0",
		"@phylum/webpack": "^4.0.2"
	},
	"author": "Max J. Polster",
	"license": "MIT"
}
